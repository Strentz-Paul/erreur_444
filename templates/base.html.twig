<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Eunnow | {{ 'global.home_label'|trans }}{% endblock %}</title>
        <link rel="icon" href="{{ asset('favicon.ico') }}">
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}
        {% block meta %}
        {% endblock %}
    </head>
    <body>
        <div class="container-fluid">
            {% block menu %}
                {% include 'layouts/front--menu.html.twig' %}
            {% endblock %}
            {% block body %}
            {% endblock %}
        </div>
        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
            <script>
                $(function () {
                    $('[data-toggle="tooltip"]').tooltip()
                })
            </script>
        {% endblock %}
    </body>
</html>
